{
    "name": "eddiriarte/console-select",
    "description": "A fancy selection interface for symfony's console component.",
    "keywords": ["library", "symfony", "console", "cli", "select", "checkbox", "radio"],
    "type": "library",
    "authors": [
        {
            "name": "eddiriarte",
            "email": "eddiriarte@gmail.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2.5",
        "symfony/console": "^5.0",
        "illuminate/support": ">=6.0",
        "illuminate/console": ">=6.0"
    },
    "require-dev": {
        "php-mock/php-mock-phpunit": "^2.6",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "EddIriarte\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html=builds/phpunit/ --log-junit builds/logs/phpunit.xml",
        "code-coverage": "phpunit --coverage-html=builds/phpunit/ --log-junit builds/logs/phpunit.xml"
    }
}
